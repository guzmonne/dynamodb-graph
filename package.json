{
  "name": "dynamodb-graph",
  "version": "4.0.0",
  "main": "dist/index.js",
  "repository": {
    "github": "git@github.com:guzmonne/dynamodb-graph.git"
  },
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.169.0",
    "cuid": "^1.3.8",
    "hex-2-num": "^1.0.0",
    "lodash": "^4.17.4"
  },
  "scripts": {
    "validate:stack": "cd scripts && ./validate_stack.sh",
    "delete:stack": "cd scripts && ./delete_stack.sh",
    "describe:stack": "cd scripts && ./describe_stack.sh",
    "deploy:stack": "cd scripts && ./deploy_stack.sh",
    "test": "jest --watch",
    "docs": "./node_modules/.bin/gulp typedoc",
    "deploy": "gh-pages -d docs",
    "docs:deploy": "yarn docs && git commit -am \"updated docs\" && yarn deploy"
  },
  "devDependencies": {
    "console.table": "^0.9.1",
    "csvtojson": "^1.1.9",
    "jest": "^21.2.1",
    "rxjs": "^5.5.6",
    "sinon": "^4.1.3",
    "yamljs": "^0.3.0",
    "@types/jest": "^22.1.2",
    "@types/lodash": "^4.14.102",
    "@types/node": "^9.4.5",
    "@types/sinon": "^4.1.3",
    "gh-pages": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-typedoc": "^2.2.0",
    "ts-jest": "^22.0.4",
    "typedoc": "^0.10.0",
    "typescript": "^2.7.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  }
}
